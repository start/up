{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "outDir": "built",
    "rootDir": "src",
    "sourceMap": true
  },
  "exclude": [
    "node_module",
    "typings/browser",
    "typings/browser.d.ts"
  ]
}